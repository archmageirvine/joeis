       <BugCode name="SEO"/>
     </Match>
     <Match classregex="gebhardt.Canonical">
       <BugCode name="SEO"/>
     </Match>

     <!-- I am allowed to ignore return values and have absolute paths in a test -->
     <Match classregex=".*Test">
       <BugCode name="RV"/>
     </Match>
     <Match classregex=".*Test">
       <BugCode name="DMI"/>
     </Match>

     <!-- false-positive -->
     <Match classregex="irvine.oeis.a008.A008866">
       <BugCode name="FCBL"/>
