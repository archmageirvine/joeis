       <BugCode name="EI"/>
     </Match>
     <Match classregex="irvine.oeis.HolonomicRecurrence">
       <BugCode name="EI"/>
     </Match>

     <!-- Don't care about serialization in the OEIS -->
     <Match classregex="irvine.oeis.*">
       <BugCode name="Se"/>
     </Match>

